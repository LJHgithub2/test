@font-face{
	font-family: 'Pretendard';
	src:url('../font/PretendardVariable.woff2') format('woff2'),
	    url('../font/PretendardVariable.woff2') format('truetype');
}
body{ 
	font-family:'pretendard';
	background-image: url("../image/top_bg.png");
	background-repeat: no-repeat;
	background-size:100vw 100vh;
}
header{
	height: 100px;
	margin: 8px 16px 0px;
	line-height: 40px;
}
header #logo{
	float: left;
}
header #logo h1{
	font-size: 0px;
	background-image: url("../image/logo.png");
	background-size: 146px 68px;
	background-repeat: no-repeat;
	background-position: center;
	display: inline-block;
	width: 180px;
	height: 80px;
}

.contain{ 
	background-color:rgba(133,323,323,0.5);
	margin:0px auto;
	width:80vw;
	height:70vh;
	/*  column-count:2;
	column-gap:32px;
	column-width:250px;*/
	
}
form{ 
	height:100%; 
}

table{ 
	float:left; 
	height:100%;
	width:50%;
}
table .td_center{ 
	text-align:center;
}

.terms_box{ 
	text-align:center;
	width:49%;
	height:100%;
	float:left;
}
.input_div span{ 
	display:block;
	margin:0 0 2% 1%;
	font-weight:bold;
	color:gray;
}
.check_text{ 
	font-size:0.8em;
	color:black;
}
.input_div{ 
	margin:0 0 0 3%; 
}
form input{ 
	box-shadow:2px 2px 2px 2px transparent;
	transition: box-shadow 0.3s;
	width:90%;
	border-radius:5px; 
	padding:0.8em;
	border:none;
}
form input:focus{ 
	outline:none;
	box-shadow:0 0 10px red;
	border-radius:5px; 
}
form input[type=text]{ 
}
form input[type=submit]{ 
	width:50%;
	height:50%;
	margin:0px;
	position:relative;
	border:1px solid darkred;
	background:gray;
	color:darkred;
}
form input[type=submit]:disabled{ 
	background:gray;
	opacity:0.5;
}
form input[type=submit]:enabled:hover{ 
	cursor:pointer;
	background:black;
}
.terms{ 
	border:5px double fuchsia;
	width:100%;
	height:80%;
	background-color:wheat;
}
.terms .title{ 
	border:1px solid black;
	width:100%;
	height:15%;
}
.terms .subtitle{ 
	display:table-cell;
	border:1px solid black;
	width:100%;
	height:5rem;
	font-size:0.8rem;
	vertical-align:middle;
}
.terms .content{ 
	border:1px solid black;
	width:100%;
	height:65%;
	font-size:19px;
	overflow:auto;
	line-height:3;
	color:black;
}
.terms_checkbox{ 
	margin:8% 0px;
	width:15px;
}

